{
  "name": "idumb-v2",
  "version": "2.2.0",
  "description": "Intelligent Delegation Using Managed Boundaries â€” an AI knowledge work platform built on OpenCode",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "idumb-v2": "./bin/cli.mjs"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "bin",
    "src/**/*.ts",
    "!src/dashboard/frontend",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "tsx tests/init.test.ts && tsx tests/persistence.test.ts && tsx tests/task.test.ts && tsx tests/delegation.test.ts && tsx tests/planning-registry.test.ts && tsx tests/work-plan.test.ts && tsx tests/task-graph.test.ts && tsx tests/plan-state.test.ts && tsx tests/smoke-code-quality.ts && tsx tests/sqlite-adapter.test.ts",
    "dev:app": "vite --config app/vite.config.ts",
    "build:app": "vite build --config app/vite.config.ts",
    "preview:app": "vite preview --config app/vite.config.ts",
    "typecheck:app": "tsc --noEmit -p tsconfig.app.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "opencode",
    "ai-governance",
    "agent",
    "llm",
    "knowledge-work",
    "multi-agent",
    "context-management"
  ],
  "repository": {
    "type": "git",
