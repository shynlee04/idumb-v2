    "@tanstack/react-start": "^1.159.5",
    "better-sqlite3": "^11.10.0",
    "chokidar": "^4.0.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lucide-react": "^0.474.0",
    "open": "^10.1.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
